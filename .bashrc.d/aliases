alias vim="nvim"
alias g="git"
alias l="eza -1 --icons --group-directories-first"
alias ls="eza"
alias ll="eza -l --git --group-directories-first"
alias lll="eza -lah --icons --git --group-directories-first"
alias open="xdg-open"
alias dotfiles='/usr/bin/git --git-dir=$HOME/dotfiles/ --work-tree=$HOME'
