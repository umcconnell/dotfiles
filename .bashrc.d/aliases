alias vim="nvim"
alias g="git"
alias l="exa -1 --icons --group-directories-first"
alias ls="exa"
alias ll="exa -l --git --group-directories-first"
alias lll="exa -lah --icons --git --group-directories-first"
alias open="xdg-open"
alias dotfiles='/usr/bin/git --git-dir=$HOME/dotfiles/ --work-tree=$HOME'
